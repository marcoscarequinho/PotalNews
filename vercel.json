{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/vercel.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/login.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/login",
      "dest": "/api/login.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}